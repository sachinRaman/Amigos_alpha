<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.amigos.sindhusha.activity.TopicTagsActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="29dp"
        android:layout_marginEnd="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Select the things you hate"
        android:textSize="20sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_constraintRight_creator="1" />

    <com.amigos.sindhusha.vo.TagView xmlns:tagview="http://schemas.android.com/apk/res-auto"
        android:id="@+id/tag_group"
        android:layout_width="0dp"
        android:layout_height="693dp"
        android:layout_margin="10dp"
        tagview:lineMargin="5dp"
        tagview:tagMargin="5dp"
        tagview:textPaddingBottom="5dp"
        tagview:textPaddingLeft="8dp"
        tagview:textPaddingRight="8dp"
        tagview:textPaddingTop="5dp"
        android:layout_marginTop="8dp"
        tagview:layout_constraintTop_toBottomOf="@+id/textView5"
        android:layout_marginRight="8dp"
        tagview:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        tagview:layout_constraintLeft_toLeftOf="parent" />
</android.support.constraint.ConstraintLayout>
